{
  "mcpServers": {
    "copilotkit": {
      "type": "sse",
      "url": "https://mcp.copilotkit.ai/sse"
    },
    "astrodocs": {
      "type": "http",
      "url": "https://mcp.docs.astro.build/mcp"
    },
    "github": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-github"],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_PERSONAL_ACCESS_TOKEN}"
      }
    }
  }
}
